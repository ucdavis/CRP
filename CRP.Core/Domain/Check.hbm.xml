<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    assembly="CRP.Core" namespace="CRP.Core.Domain">
  <class
    name="Check"
    table="Checks" >
    <id name="ID" access="nosetter.lowercase" column="id" unsaved-value="0">
      <generator class="identity" />
    </id>

    <property name="Payee" column="Payee" />
    <property name="CheckNumber" column="CheckNumber" />
    <property name="Amount" column="Amount" />
    <property name="DateReceived" column="DateReceived" />

  </class>
</hibernate-mapping>