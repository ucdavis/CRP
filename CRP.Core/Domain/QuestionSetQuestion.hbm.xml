<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    assembly="CRP.Core" namespace="CRP.Core.Domain">
  <class
    name="QuestionSetQuestion"
    table="QuestionSetQuestions" >
    <id name="ID" access="nosetter.lowercase" column="id" unsaved-value="0">
      <generator class="identity" />
    </id>

    <many-to-one name="QuestionSet" class="CRP.Core.Domain.QuestionSet" column="QuestionSetId" cascade="none" fetch="join" not-null="true" />
    <many-to-one name="Question" class="CRP.Core.Domain.Question" column="QuestionId" cascade="none" fetch="join" not-null="true" />

    <property name="Order" column="[Order]" />
    
  </class>
</hibernate-mapping>